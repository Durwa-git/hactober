#include <bits/stdc++.h> 
using namespace std; 
  //header file
int main() 
{ 
    int x, y; ///add comment
    
    printf("Enter Value of x "); 
    scanf("%d", &x); 
    printf("\nEnter Value of y "); 
    scanf("%d", &y); 
    swap(x, y); 
    printf("\nAfter Swapping: x = %d, y = %d", x, y); 
    return 0; 
} 
